<?xml version="1.0" encoding="UTF-8"?>
<WebConfig>
<context scanpackages="com.ycframe;YC.Frame"></context> 
<webparams>
	<param name="servername" value="ycframe" />  
	<param name="SessionGroup" value="ycframe" /> 
	<param name="CacheTimoutInterval" value="1" />
	<param name="SessionTimoutInterval" value="6000" /> 
	
	<param name="UrlAuthorization" value="com.ycframe.web.security.WebUrlAuthorization" /> 
	<param name="Authentication" value="com.ycframe.web.security.WebAuthentication" /> 
	<param name="Authorization" value="com.ycframe.web.security.WebAuthorization" /> 
		
</webparams>
<weblisteners> 
</weblisteners> 
<webfilters> 
	<bean class="com.ycframe.web.ex.filter.EncodeFilter" urlpattern="/*" errorpage="/">
		<param name="encode" value="UTF-8" />
		<param name="enabled" value="true" /> 
		<param name="nofilter" value="*.no" />			
	</bean>  
</webfilters> 
<webdos> 
</webdos>
<renders>
	<render result="com.ycframe.web.context.result.JsonResult" render="com.ycframe.web.context.render.JsonRender" />
</renders>
</WebConfig>